{
  "name": "eosc-portal-commons-components",
  "version": "0.0.1",
  "description": "Library containing the custom UI components of the EOSC services. Uses the JS scripts, and the SCSS styles for consistent visualization and events triggering.",
  "scripts": {
    "start": "gulp serve",
    "deploy:stable": "gulp build_lib --mode production --dist_path stable && gulp deploy_lib --dest_branch stable --dist_path stable --git_paths_to_include README.md",
    "deploy:latest": "gulp build_lib --mode production --dist_path latest && gulp deploy_lib --dest_branch latest --dist_path latest --git_paths_to_include README.md",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyfronet-fid/eosc-portal-commons-components.git"
  },
  "keywords": [
    "EOSC",
    "common",
    "components"
  ],
